{
  "name": "mocode/flysystem-aliyunoss",
  "description": "Flysystem adapter for the Aliyun Oss storage.",
  "authors": [
    {
      "name": "mocode",
      "email": "mo@mocode.cn"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.5.9",
    "league/flysystem": "^1.0",
    "aliyuncs/oss-sdk-php": "~2.3"
  },
  "require-dev": {
    "php": ">=5.6.0",
    "phpunit/phpunit": "~8.0"
  },
  "autoload": {
    "psr-4": {
      "Mocode\\Flysystem\\AliyunOss\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mocode\\Flysystem\\AliyunOss\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mocode\\Flysystem\\AliyunOss\\AliyunOssServiceProvider"
      ]
    }
  }
}